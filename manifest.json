{
  "manifest_version": 2,

  "name": "Duolingo reminder",
  "description": "Shows a reminder to practice duolingo",
  "version": "1.0",

  "permissions": [
    "alarms",
    "https://www.duolingo.com/*",
    "tabs"
  ],
  "content_scripts": [{
    "matches": ["https://www.duolingo.com/*"],
    "js": ["index.js"]
  }]
}
